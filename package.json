{
  "name": "api.EazyGoRwanda.v1.0.0",
  "version": "2.0.0",
  "description": "EazyGoRwanda App APIs",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate": "npx sequelize-cli db:migrate ",
    "rollback": "npx sequelize-cli db:migrate:undo ",
    "rollback:all": "npx sequelize-cli db:migrate:undo:all ",
    "seed": "npx sequelize-cli db:seed:all ",
    "rollback:seed": "npx sequelize-cli db:seed:undo  ",
    "rollback:seed:all": "npx sequelize-cli db:seed:undo:all  ",
    "prepare": "husky install && npx husky add .husky/pre-commit \"npx lint-staged\"",
    "format:write": "prettier \"**/*.{js,ts}\" --write",
    "format:check": "prettier \"**/*.{js,ts}\" --check"
  },
  "lint-staged": {
    "**/*.{js,ts}": "format:write"
  },
  "author": "Alvin Coder",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mailgen": "^2.0.28",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "qrcode": "^1.5.3",
    "sequelize": "^6.35.1",
    "socket.io": "^4.7.4",
    "vcards-js": "^2.10.0",
    "vcf": "^2.1.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.6.2"
  }
}
